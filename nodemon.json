{
    "ignore": [
        ".git",
        "node_modules/**/node_modules"
    ],
    "verbose": true,
    "delay": 2000,
    "execMap": {
        "js": "node --harmony",
        "ts": "ts-node --files"
    },
    "watch": [
        "server/"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "ts"
}
